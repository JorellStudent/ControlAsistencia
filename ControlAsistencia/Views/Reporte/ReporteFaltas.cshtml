@{
    Layout = "_Layout";
    ViewData["Title"] = "Reporte de Faltas";
}

<h2>Reporte de Faltas</h2>

<table class="table">
    <thead>
        <tr>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Correo</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var usuario in Model)
        {
            <tr>
                <td>@usuario.Nombre</td>
                <td>@usuario.Apellido</td>
                <td>@usuario.Correo</td>
            </tr>
        }
    </tbody>
</table>

<a href="/Reporte">Volver al inicio de reportes</a>
